{
  "name": "node-template-typescript",
  "version": "1.0.0",
  "description": "Node Modules Template (TypeScript)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "start": "npm run build && node lib/index",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piyawat37/node-template-typescript.git"
  },
  "keywords": [],
  "author": "Piyawat Pewmattana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piyawat37/node-template-typescript/issues"
  },
  "homepage": "https://github.com/piyawat37/node-template-typescript#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/multer": "^1.4.2",
    "@types/request": "^2.48.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "routing-controllers": "^0.8.1-alpha.2",
    "typeorm": "^0.2.25"
  }
}
